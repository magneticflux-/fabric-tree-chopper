{
  "schemaVersion": 1,
  "id": "fabric-tree-chopper",
  "version": "${version}",
  "name": "Fabric Tree Chopper",
  "description": "A Fabric mod to make cutting down trees easier and more realistic. | Ported to 1.20.x by Duzo",
  "authors": [
    "magneticflux-",
    "ft-l"
  ],
  "contact": {
    "homepage": "https://github.com/magneticflux-/fabric-tree-chopper",
    "sources": "https://github.com/magneticflux-/fabric-tree-chopper"
  },
  "license": "LGPL-3.0-or-later",
  "icon": "assets/fabric-tree-chopper/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.skaggsm.treechoppermod.FabricTreeChopper"
      }
    ],
    "modmenu": [
      "com.skaggsm.treechoppermod.FabricTreeChopperConfigScreen"
    ]
  },
  "mixins": [
    "fabric-tree-chopper.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.4.0",
    "fabric": "*",
    "fabric-language-kotlin": ">=1.3.30"
  },
  "suggests": {
  }
}
